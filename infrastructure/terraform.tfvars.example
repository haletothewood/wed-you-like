# Vercel Configuration
# Get token from: https://vercel.com/account/tokens
vercel_api_token = "your-vercel-api-token"

# Get project ID from: Vercel Dashboard → Project → Settings → General
vercel_project_id   = "prj_xxxxxxxxxxxxxxxxxxxx"
vercel_project_name = "wed-you-like"

# Turso Database
# Get from: turso db show wed-you-like
turso_database_url = "libsql://your-database.turso.io"
# Get from: turso db tokens create wed-you-like
turso_auth_token = "your-turso-auth-token"

# Resend Email
# Get from: https://resend.com/api-keys
resend_api_key = "re_your-resend-api-key"

# Application
base_url = "https://your-domain.com"
